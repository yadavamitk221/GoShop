{
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "build/**",
      "use": "@vercel/static"
    }
  ],

  "routes": [
    { 
      "src": "/", 
      "dest": "./build/index.html" 
    }, 
    { 
      "src": "/products", 
      "dest": "./build/index.html" 
    }, 
    { 
      "src": "/products/(.*)", 
      "dest": "./build/index.html" 
    },
    { 
      "src": "/categories", 
      "dest": "./build/index.html" 
    }, 
    { 
      "src": "/categories/(.*)", 
      "dest": "./build/index.html" 
    },
    { 
      "src": "/brands", 
      "dest": "./build/index.html" 
    }, 
    { 
      "src": "/brands/(.*)", 
      "dest": "./build/index.html" 
    },
    { 
      "src": "/user", 
      "dest": "./build/index.html" 
    }, 
    { 
      "src": "/users/(.*)", 
      "dest": "./build/index.html" 
    },
    { 
      "src": "/auth", 
      "dest": "./build/index.html" 
    }, 
    { 
      "src": "/auth/(.*)", 
      "dest": "./build/index.html" 
    },
    { 
      "src": "/cart", 
      "dest": "./build/index.html" 
    }, 
    { 
      "src": "/cart/(.*)", 
      "dest": "./build/index.html" 
    },
    { 
      "src": "/orders", 
      "dest": "./build/index.html" 
    }, 
    { 
      "src": "/orders/(.*)", 
      "dest": "./build/index.html" 
    },
    { 
      "src": "/webhook", 
      "dest": "index.js" 
    }, 
    { 
      "src": "/create-payment-intent", 
      "dest": "index.js" 
    }, 
    
    { 
      "src": "/(.+)", 
      "dest": "build/$1" 
    }
  ]
}
